.facebook_outer
  display -moz-box
  -moz-box center
  -moz-box-align center

  display -webkit-box
  -webkit-box-pack center
  -webkit-box-align center

  display box
  box-pack center
  box-align center

.facebook_link
  font-family Allura
  font-style normal
  font-size 28px
  text-align center
  width 50%
  margin 0 auto

.facebook_link a:link
  color black
  text-decoration none
  target _blank

.facebook_link a:visited
  color black
  text-decoration none

.goods_text
